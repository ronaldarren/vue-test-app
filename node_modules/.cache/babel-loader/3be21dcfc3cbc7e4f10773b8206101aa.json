/v-card-actions>\r\n\r\n                        <!-- <v-card-text>\r\n            <v-form ref=\"form\" v-model=\"valid\" lazy-validation>\r\n              <v-text-field\r\n                prepend-icon=\"person\"\r\n                name=\"email\"\r\n                label=\"Email\"\r\n                type=\"email\"\r\n                v-model=\"email\"\r\n                :rules=\"emailRules\"\r\n                required\r\n              ></v-text-field>\r\n              <v-text-field\r\n                prepend-icon=\"lock\"\r\n                name=\"password\"\r\n                label=\"Password\"\r\n                id=\"password\"\r\n                type=\"password\"\r\n                required\r\n                v-model=\"password\"\r\n                :rules=\"passwordRules\"\r\n              ></v-text-field>\r\n            </v-form>\r\n          </v-card-text>\r\n          <v-card-actions>\r\n            <v-spacer></v-spacer>\r\n            <v-btn color=\"primary\" :disabled=\"!valid\" @click=\"submit\">Join</v-btn>\r\n          </v-card-actions>-->\r\n                    </v-form>\r\n                </v-card>\r\n            </v-flex>\r\n        </v-layout>\r\n    </v-container>\r\n<!-- </div> -->\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n    name: 'Join',\r\n    data() {\r\n        return {\r\n            step: 1,\r\n            valid: false,\r\n            email: '',\r\n            password: '',\r\n            confirmPassword: '',\r\n            emailRules: [\r\n                v => !!v || 'E-mail is required',\r\n                v => /.+@.+/.test(v) || 'E-mail must be valid'\r\n            ],\r\n            passwordRules: [\r\n                v => !!v || 'Password is required',\r\n                v =>\r\n                    v.length >= 6 ||\r\n                    'Password must be greater than 6 characters'\r\n            ],\r\n            confirmPasswordRules: [\r\n                v => !!v || 'Confirm your password',\r\n                v =>\r\n      